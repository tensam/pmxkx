#alias {set_force_desc} {
	#list {l_startlist} {clear};
	#list {l_endlist} {clear};
	#foreach {$skill_list[%*]} {l_skill} {
		#if {&{force_desc[$l_skill[name]]} != 0} {
			#list {specialforce} {add} {$l_skill[name]};
			#variable l_start {$force_desc[$l_skill[name]][dazuo_start]};				
			#variable l_end {$force_desc[$l_skill[name]][dazuo_end]};
			#list {l_startlist} {find} {$l_start} {l_find};
			#if {$l_find == 0} {#list {l_startlist} {add} {$l_start}};
			#list {l_endlist} {find} {$l_end} {l_find};
			#if {$l_find == 0} {#list {l_endlist} {add} {$l_end}};
		};
	};
	#if {&specialforce[] == 0} {#showme {Please set force description in variable: dazuo_start and dazuo_end, or add force_desc in init.tin}};
	#else {
		#if {&{dazuo_start} != 0 && "$dazuo_start" != ""} {
			#echo {Inherit variable dazuo_start from character's tin!!!};
		};
		#else {
			#variable dazuo_start {$l_startlist};
			#list dazuo_start simplify; 
			#if {&l_startlist[]>1} {
				#replace {dazuo_start} {;} {|};
				#variable dazuo_start {{$dazuo_start}};
			};
		};
		#if {&{dazuo_end} != 0 && "$dazuo_end" != ""} {
			#echo {Inherit variable dazuo_end from character's tin!!!};
		};
		#else {
			#variable dazuo_end {$l_endlist};
			#list dazuo_end simplify; 
			#if {&l_endlist[]>1} {
				#replace {dazuo_end} {;} {|};
				#variable dazuo_end {{$dazuo_end}};
			};
		};
		#showme {Force setting is done!};
	};
	#unvariable l_%*;
}

#alias {set_busy_desc} {
	#list {l_successlist} {clear};
	#list {l_faillist} {clear};
	#foreach {$skill_list[%*]} {l_skill} {
		#if {&busy_desc[$l_skill[name]] != 0} {
			#variable l_success {$busy_desc[$l_skill[name]][success]};				
			#variable l_fail  {$busy_desc[$l_skill[name]][fail]};
			#list {l_successlist} {find} {$l_success} {l_find};
			#if {0 == $l_find} {#list {l_successlist} {add} {$l_success}};
			#regex {$l_fail} {^$} {
			} {
				#list {l_faillist} {find} {$l_fail} {l_find};
				#if {0 == $l_find} {#list {l_faillist} {add} {$l_fail}};
			};
		};
	};
	#if {&l_successlist[] == 0} {#showme {Please set busy description in variable: busy_success and busy_fail, or add busy_desc in init.tin}};
	#else {
		#if {&{busy_success} !=0 && "$busy_success" != ""} {
			#echo {Inherit variable busy_success from character's tin!!!};
		};
		#else {
			#variable busy_success {$l_successlist};
			#list busy_success simplify;
			#if {&l_successlist[] == 1} {#replace busy_success {.} {%}};
			#else {
				#replace {busy_success} {;} {|};
				#variable {busy_success} {{$busy_success}};
			};
		};
		#if {&{busy_fail} !=0 && "$busy_fail" != ""} {
			#echo {Inherit variable busy_fail from character's tin!!!};
		};
		#else {
			#if {&l_faillist[]>0} {
				#variable busy_fail {$l_faillist};
				#list busy_fail simplify;
				#if {&l_faillist[] == 1} {#replace busy_fail {.} {%}};
				#else {
					#replace {busy_fail} {;} {|};
					#variable {busy_fail} {{$busy_fail}};
				};
			};
			#else {#variable busy_fail {}};
		};
		#showme {Busy setting is done!};
	};
	#unvariable l_%*;
}

