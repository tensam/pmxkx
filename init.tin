#alias {init.do} {
	updatedolist init;
	#class {cVariables} {read} {$MOD/cVariables.tin};	
	#if {"$me[ename]" != ""} {#class {cVariables} {read} {$VAR/${me[ename]}_Vars.tin}};
	#var mission_status;
	#if {&weapons[] > 0} {
		#foreach {$weapons[%*]} {l_item} {
			#list {keep_equips} {add} {$l_item};
		}
	};
	#if {&{int_weapon} != 0} {
		#list {keep_equips} {find} {$int_weapon} {l_find};
		#if {$l_find == 0} {#list {keep_equips} {add} {$int_weapon}};
	};
	#if {&equips[] > 0} {
		#foreach {$equips[%*]} {l_item} {
			#list {keep_equips} {add} {$l_item};
		}
	};
	#if {&int_equips[] > 0} {
		#foreach {$int_equips[%*]} {l_item} {
			#list {keep_equips} {find} {$l_item} {l_find};
			#if {$l_find == 0} {#list {keep_equips} {add} {$l_item}};
		}
	};
	#if {&keep_equips[] > 0} {
		#list {keep_equips} {find} {piaomiao xunzhang} {l_find};
		#if {$l_find != 0} {ixunzhang};
	};	
	#unvariable l_find;
	add_option_traps;
	score;
	jingjie;
	id here;
	#class {status} read $MOD/status.tin; 
	hp;
	iskills;
	wuxue;
	#delay {5} {
		#class {cVariables} {read} {$MOD/cVariables.tin};	
		#if {"$me[ename]" != ""} {#class {cVariables} {read} {$VAR/${me[ename]}_Vars.tin}};
		#var mission_status;
	};
	#delay {3} {
		load_option_mods;
		#if {&skill_list[基本内功] == 0} {#variable {noforce} {1}};
		#if {$skill_list[本草术理][level] > 30} {#variable {me[healable]} {1}};
