#nop ===========================Action===========================
#nop -------------------------score info-------------------------				
#action {^┃%s姓    名：%S%s┃%s力量：「%s%d/%d%s」%s才智：「%s%d/%d%s」%s┃} {
	#variable me[cname] %2;
	#variable me[str] %6;
	#variable me[eff_str] %7;
	#variable me[int] %11;
	#variable me[eff_int] %12;
}

#action {^┃%s门    派：%S%s┃%s体质：「%s%d/%d%s」%s身法：「%s%d/%d%s」%s┃} {	
	#variable me[family] %2;
	#variable me[con] %6;
	#variable me[eff_con] %7;
	#variable me[dex] %11;
	#variable me[eff_dex] %12;
}	

#action {^┃级    别：%S%s潜} {
	#variable {me[lvl]} {@ctoi{%1}};
}
				
#action {授业恩师：{[\x80-\xff]+}%*┃} {
	#if {"$me[cmaster]" != "%1"} {
		#variable me[cmaster] %1;
		#if {"%1" != "尚未拜师"} {
			#action {$me[cmaster](%*)}	{
				#regex {$me[cmaster]} {^$} {} {
					#format {me[emaster]} {%l} {%%1};
					pat $me[emaster];
					#regex {$master} {^$} {#variable master $me[emaster]};
					#unaction {$$me[cmaster](%*)}
				}
			}
		}
	}
}

#action {^┃性    别：%*性%s┗} {
	#if {"%1" == "女"} {#variable me[gender] {female}};
	#elseif {"%1" == "男"} {#variable me[gender] {male}};
	#else {#variable me[gender] {none}};
}

#action {^${me[cname]}%s=%s%+} {
	#regex {$me[cname]} {^$} {} {
		#variable me[ename] %3;
		#unaction {^$${me[cname]}%s=%s%+};
	};
}
	
#nop -------------------------skills info------------------------
#action {^%?%?你目前所掌握的技能［共%*种］} {
	#list {skill_list} clr;
	#variable cnt 0;
	#action {^%%0} {
		#regex {%%0} {{★|}%S (%*)%s- %S%s%d/} {
			#variable skill_list[&2][name] &3;
			#variable skill_list[&2][level] &7;
			#variable skill_e2c[&3] &2;
		};
		#regex {%%0} {%*──────%*} {
			#math {cnt} {$cnt+1};
			#if {$cnt >= 2} {
				#unaction {^%%%0};
				#unvariable cnt;
			};
		};
	};
}


#nop --------------------jingjie info------------------------
#action {^%?%?你当前武学境界：【 %1 】}{
	#echo %1;
	#switch {"%1"} {
		#case {"炼体养精"} {#var jingjie 0};
		#case {"炼精化气"} {#var jingjie 1};
		#case {"炼气化神"} {#var jingjie 2};
		#default {#var jingjie 3}
	};
}

#nop ------------------------向阳老宅铜钱------------------------
#action {^%?%?林震南低声说：先祖去世前，曾给家父留下%*个} {
	#variable {xy_coins} {@ctoi{%3}};
	#showme $xy_coins;
}

#nop -------------------------stage info-------------------------
#action {^%?%?Force setting is done!} {
	#if {&{dummy} == 0} {
		set_busy_desc;
		#if {&{sx_go} == 0} {set_sx_go};
		#if {&{xy_coins} == 0} {ask_coin};		
		#delay {1} {
			#format {conWeekday} {%t} {%u};
			#if {$conWeekday != $zapWeekday} {#echo a new day,all status reset;mission_status.reset;#var mission_status};
			#else {#echo the same day~keep status};
			#if {&{biwu_mate} != 0} {#math {biwu_delay} {$xy_coins * 24 * 36 / 100}};
			#if {$bInit == 0} {
				ask_ghost;
				#delay {1} {ask_gcmap};
				#if {$me[comb_exp]>2000000 && $me[yl_exp]>50000} {
					#delay {4} {ask_shanpai}
				};
				#delay {7} {
					#class init kill;
					#if {$bYaopai} {
						fly lz;w;#2 n;e;ask nu erhai about 腰牌;
						#delay {init_finish} {fly wm;w} {2};
					};
					#if {"$me[family]" == "华山派" && $me[comb_exp]>3700000} {
						fly ws;sd;#2 su;enter;
						wield sword;break wall;
						enter;#2 wu;bai feng;
						#delay {init_finish} {fly wm;w} {2};
					};
					#else {fly wm;w};
					#variable bInit 1;
				}
			};
			#else {
				#class init kill;
				#if {$bDrunk == 1} {#delay {20} {fly wm;w}} {fly wm;w};
			};
		}
	};
	#else {
		#class init kill;
	};
}

